<p id="notice"><%= notice %></p>

<h1>Listing Position Papers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @position_papers.each do |position_paper| %>
      <tr>
        <td><%= link_to 'Show', position_paper %></td>
        <td><%= link_to 'Destroy', position_paper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Position paper', new_position_paper_path %>
